<mxEditor defaultGroup="group" defaultEdge="connector">
	<include name="config/editor-commons.xml"/>
	<add as="onInit"><![CDATA[
		function ()
		{
			onInit(this);
		}
	]]></add>
	<ui>
		<add as="graph" element="graph"/>
		<add as="status" element="status"/>
		<add as="toolbar" element="toolbar"/>
	</ui>
	<Array as="templates">
		<add as="group">
			<Group label="" href="">
				<mxCell vertex="1" style="group" connectable="0"/>
			</Group>
		</add>
		<add as="connector">
			<Connector label="" href="">
				<mxCell edge="1">
					<mxGeometry as="geometry" relative="1"/>
				</mxCell>
			</Connector>
		</add>
		<add as="container">
			<Container label="Container" href="">
				<mxCell vertex="1" style="swimlane" connectable="0">
					<mxGeometry as="geometry" width="500" height="500"/>
				</mxCell>
			</Container>
		</add>
		<add as="text">
			<Text label="Text Here" href="">
				<mxCell vertex="1" style="text">	
					<mxGeometry as="geometry" width="60" height="20"/>
				</mxCell>
			</Text>
		</add>

		<!-- production line items -->
		<add as="ConveyorBelt">
			<Image label="" href="">
				<mxCell vertex="1" style="conveyor-belt">
					<mxGeometry as="geometry" width="150" height="150" type="component"/>
				</mxCell>
			</Image>
		</add>
		<add as="Delivery">
			<Image label="" href="">
				<mxCell vertex="1" style="delivery">
					<mxGeometry as="geometry" width="150" height="150" type="component"/>
				</mxCell>
			</Image>
		</add>
		<add as="FillingArm">
			<Image label="" href="">
				<mxCell vertex="1" style="filling-arm">
					<mxGeometry as="geometry" width="150" height="150" type="component"/>
				</mxCell>
			</Image>
		</add>
		<add as="RoboticArm">
			<Image label="" href="">
				<mxCell vertex="1" style="robotic-arm">
					<mxGeometry as="geometry" width="150" height="150" type="component"/>
				</mxCell>
			</Image>
		</add>
		<add as="WorkingMan">
			<Image label="" href="">
				<mxCell vertex="1" style="working-man">
					<mxGeometry as="geometry" width="150" height="150" type="component"/>
				</mxCell>
			</Image>
		</add>

		<!--temperature-and-humidity-sensors-->
		<add as="BME280">
			<Image label="" href="">
				<mxCell vertex="1" style="BME280">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>
		<add as="DHT11">
			<Image label="" href="">
				<mxCell vertex="1" style="DHT11">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>
		<add as="DS18B20">
			<Image label="" href="">
				<mxCell vertex="1" style="DS18B20">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>
		<add as="MLX90614">
			<Image label="" href="">
				<mxCell vertex="1" style="MLX90614">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>

		<!--ultrasonic-sensors-->
		<add as="MLX90614">
			<Image label="" href="">
				<mxCell vertex="1" style="MLX90614">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>
		<add as="QTR-3RC">
			<Image label="" href="">
				<mxCell vertex="1" style="QTR-3RC">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>
		<add as="SRF02">
			<Image label="" href="">
				<mxCell vertex="1" style="SRF02">
					<mxGeometry as="geometry" width="80" height="80" type="sensor"/>
				</mxCell>
			</Image>
		</add>

	</Array>
	<mxGraph as="graph" alternateEdgeStyle="verticalConnector" allowLoops="1" dropEnabled="1">
		<add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
		<add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
		<add as="getTooltipForCell"><![CDATA[
			function(cell)
			{
				var label = cell.getAttribute('label');
				var style = cell.getStyle();
			
				return '';
			}
		]]></add>
		<add as="convertValueToString"><![CDATA[
			function(cell)
			{
				return cell.getAttribute('label');
			}
		]]></add>
		<mxStylesheet as="stylesheet">
			<add as="text">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="fontSize" value="25"/>
				<add as="shadow" value="0"/>
				<add as="strokeColor" value="none"/>
				<add as="fillColor" value="white"/>
				<add as="gradientColor" value="none"/>
			</add>
			<add as="defaultVertex" extend="text">
				<add as="shape" value="rectangle"/>
				<add as="align" value="center"/>
				<add as="verticalAlign" value="middle"/>
				<add as="fontSize" value="25"/>
				<add as="labelBackgroundColor" value="white"/>
				<add as="shadow" value="1"/>
				<add as="strokeColor" value="#C3D9FF"/>
				<add as="fillColor" value="#C3D9FF"/>
				<add as="gradientColor" value="white"/>
			</add>
			<add as="group">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="fontSize" value="25"/>
				<add as="align" value="center"/>
				<add as="verticalAlign" value="middle"/>
				<add as="strokeColor" value="gray"/>
				<add as="dashed" value="0"/>
			</add>
			<add as="defaultEdge">
				<add as="shape" value="connector"/>
				<add as="fontSize" value="25"/>
				<add as="align" value="center"/>
				<add as="verticalAlign" value="middle"/>
				<add as="rounded" value="1"/>
				<add as="labelBackgroundColor" value="white"/>
				<add as="strokeColor" value="#4580e8"/>
				<add as="strokeWidth" value="3"/>
				<!--<add as="edgeStyle" value="orthogonalEdgeStyle"/>-->
				<add as="endArrow" value="classic"/>
				<!--<add as="startArrow" value="classic"/>-->
				<!--<add as="dashed" value="1"/>-->
			</add>
			<add as="verticalConnector">
				<add as="elbow" value="vertical"/>
			</add>
			<add as="straightConnector">
				<add as="shape" value="connector"/>
				<add as="endArrow" value="classic"/>
				<add as="edgeStyle">null</add>
			</add>
			<add as="arrowConnector">
				<add as="shape" value="arrow"/>
				<add as="fillColor" value="#e89a66"/>
				<add as="endSize" value="20"/>
                                <add as="fontSize" value="25"/>
				<add as="startArrow" value="classic"/>
				<remove as="edgeStyle"/>
			</add>
			<add as="swimlane">
				<add as="shape" value="swimlane"/>
				<add as="shadow" value="0"/>
				<add as="startSize" value="23"/>
				<add as="align" value="center"/>
				<add as="verticalAlign" value="top"/>
				<add as="strokeColor" value="#EEEEEE"/>
				<add as="fillColor" value="#D4D4D4"/>
			</add>

			<!-- production line items -->
			<add as="conveyor-belt">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/production-line-items/conveyor-belt.png"/>
			</add>
			<add as="delivery">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/production-line-items/delivery.png"/>
			</add>
			<add as="filling-arm">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/production-line-items/filling-arm.png"/>
			</add>
			<add as="robotic-arm">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/production-line-items/robotic-arm.png"/>
			</add>
			<add as="working-man">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/production-line-items/working-man.png"/>
			</add>

			<!--temperature-and-humidity-sensors-->
			<add as="BME280">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/temperature-and-humidity-sensors/BME280.jpg"/>
			</add>
			<add as="DHT11">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/temperature-and-humidity-sensors/DHT11.jpg"/>
			</add>
			<add as="DS18B20">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/temperature-and-humidity-sensors/DS18B20.jpg"/>
			</add>
			<add as="MLX90614">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/temperature-and-humidity-sensors/MLX90614.jpg"/>
			</add>

			<!--ultrasonic-sensors-->
			<add as="HC-SR04">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/ultrasonic-sensors/HC-SR04.jpg"/>
			</add>
			<add as="QTR-3RC">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/ultrasonic-sensors/QTR-3RC.jpg"/>
			</add>
			<add as="SRF02">
				<add as="shape" value="image"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="image" value="images/hitech/ultrasonic-sensors/SRF02.jpg"/>
			</add>

			<add as="two_way_arrow">
				<add as="shape" value="arrow"/>
				<add as="startArrow" value="classic"/>
				<remove as="edgeStyle"/>
			</add>

		</mxStylesheet>
		<mxGraphModel as="model">
			<add as="valueForCellChanged"><![CDATA[
				function(cell, value)
				{
					var previous = null;
					
					if (value == null || isNaN(value.nodeType))
					{
						previous = cell.value.getAttribute('label');

						if (value == null)
						{
							cell.value.removeAttribute('label');
						}
						else
						{
							cell.setAttribute('label', value);
						}
					}
					else
					{
						previous = cell.value;
						cell.value = value;
					}
					
					return previous;
				}
			]]></add>
			<root>
				<Diagram label="My Diagram" href="http://www.jgraph.com/" id="0">
					<mxCell/>
				</Diagram>
				<Layer label="Default Layer" id="1">
					<mxCell parent="0"/>
				</Layer>
			</root>
		</mxGraphModel>
	</mxGraph>
	<mxDefaultToolbar as="toolbar">

		<add as="Connector" template="connector" icon="images/wso2/arrow.png"/>

		<!-- production line items -->
		<add as="Conveyor Belt" template="ConveyorBelt" icon="images/hitech/production-line-items/conveyor-belt.png"/>
		<add as="Delivery" template="Delivery" icon="images/hitech/production-line-items/delivery.png"/>
		<add as="Filling Arm" template="FillingArm" icon="images/hitech/production-line-items/filling-arm.png"/>
		<add as="Robotic Arm" template="RoboticArm" icon="images/hitech/production-line-items/robotic-arm.png"/>
		<add as="Working Man" template="WorkingMan" icon="images/hitech/production-line-items/working-man.png"/>
		<hr></hr>

		<!--temperature-and-humidity-sensors-->
		<add as="BME280" template="BME280" icon="images/hitech/temperature-and-humidity-sensors/BME280.jpg"/>
		<add as="DHT11" template="DHT11" icon="images/hitech/temperature-and-humidity-sensors/DHT11.jpg"/>
		<add as="DS18B20" template="DS18B20" icon="images/hitech/temperature-and-humidity-sensors/DS18B20.jpg"/>
		<add as="MLX90614" template="MLX90614" icon="images/hitech/temperature-and-humidity-sensors/MLX90614.jpg"/>
		<hr></hr>

		<!--ultrasonic-sensors-->
		<add as="HC-SR04" template="HC-SR04" icon="images/hitech/ultrasonic-sensors/HC-SR04.jpg"/>
		<add as="QTR-3RC" template="QTR-3RC" icon="images/hitech/ultrasonic-sensors/QTR-3RC.jpg"/>
		<add as="SRF02" template="SRF02" icon="images/hitech/ultrasonic-sensors/SRF02.jpg"/>
		<hr></hr>

	</mxDefaultToolbar>
</mxEditor>
